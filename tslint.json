{
  "extends": ["tslint:recommended"],
  "defaultSeverity": "warning",
  "rules": {
    "quotemark": [true, "single"],
    "interface-name": [true, "never-prefix"]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}